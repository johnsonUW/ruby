<h1> Students</h1>
<a href="/student/new"> Create a new student</a>
 <% if @student.any? %>
  <ul>Student
    <% @student.each do |student| %>
      <li><a href="/student/<%= student.id %>"><%= student.name %></a></li>
      <% end %>
 </ul>
 
 <% else %>
  <p> No student have been created yet!</p>
 <% end %>
 <%= erb :footer %>